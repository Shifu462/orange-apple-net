model = Sequential([
    Conv2D(32, (3, 3),
        activation='relu',
        input_shape=input_shape),
    Conv2D(64, (3, 3), activation='relu'),
    MaxPooling2D(pool_size=(2, 2)),

    Dropout(0.25),
    Flatten(),

    Dense(64, activation='relu'),
    Dropout(0.5),
    Dense(2, activation='softmax')
])